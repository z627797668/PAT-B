#include<stdio.h>
#include<stdlib.h>
#include<string.h>
const int maxn = 105;
char ans[10][5] = { "ling","yi","er","san","si","wu","liu","qi","ba","jiu" };
int main()
{
	char str[maxn];
	scanf("%s", str);
	int sum = 0;
	int len = strlen(str);
	for (int i = 0; i < len; i++)
	{
		sum += str[i] - '0';
	}
	int num = 0;
	int an[maxn];
	do {
		an[num++] = sum % 10;
		sum /= 10;
	} while (sum != 0);
	for (int i = num-1; i >=0; i--)
	{
		printf("%s", ans[an[i]]);
		if (i > 0)
			printf(" ");
	}
	system("pause");
	return 0;
}
