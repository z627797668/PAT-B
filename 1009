#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
	char s[85];
	gets_s(s);
	int len = strlen(s);
	for (int i = len - 1; i >= 0;)
	{
		int j = i;
		int num = 0;
		char temp[85];
		while (s[j]!=' '&&j>=0)
		{
			temp[num++] = s[j];
			j--;
		}
		i = --j;
		for (int k = num-1;k>=0; k--)
		{
			printf("%c", temp[k]);
		}
		if (j >= 0)
			printf(" ");
	}
	system("pause");
	return 0;
}
